sequential: true
email_on_fail: false
continue_on_fail: true
skip_tasks: [_board_on_and_validate, _board_login, _board_off]
tests:
  include: [tests.empty.Test, tests.memory]
  exclude: []
  parameters:
    tests.memory.MemoryFree:
      minimum: 200000
    tests.memory.MemoryReadWrite:
      - size: 20
      # Parametrized tests don't work, because TestRunner expects unique test names
      #- size: 256
      #- size: 65000